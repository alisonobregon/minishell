array shell, array var

while var
{
    while(shell->ev)
    {
        if shell->env[] == var[]
        free shell->env[]
        shell->env[] = strdup(var[])
        let's remove that variable from var
        var = rm_str_from_array(var, var[])
        break;
    }
    ++;
    add var[] to shell->env
}